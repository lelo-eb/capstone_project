{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://reciperealm.com/recipeFavorite.schema.json",
    "type": "object",
    "properties": {
      "recipe": {
        "type": "string",
        "ref": "recipe"
      },
      "user": {
        "type": "string",
        "ref": "user"
      },
      "favorite": {
        "type": "boolean"
      }
    },
    "additionalProperties": false,
    "required": [
      "recipe",
      "user"
    ]
  }
  